{
    "extends": "../tslint.json",
    "rules": {
        "directive-selector": [
            true,
            "attribute",
            "app",
            "camelCase"
        ],
        "component-selector": [
            true,
            "element",
            "app",
            "kebab-case"
        ],
        "quotemark": [
          false,
          "single"
        ],
      "comment-format": [
        false,
        "check-space"
      ],
      "no-trailing-whitespace": false
    }
}
